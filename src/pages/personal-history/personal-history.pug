extends ../../layout/layout

block vars
    - var pageTitle = 'История заказов'
    - var pageBodyClass = ''
    - var dir = './'


block premain
    .breadcrumb-wrap
        .container
            +breadcrumb({
                href_content1: '',
                content1: 'История заказов',
                href_content2: '',
                content2: '',
                content3: ''
            })
    .container
        .row
            .col-lg-8.offset-lg-2
                h1.h1.page-title История заказов
                include ../../components/personal-nav/personal-nav
                include ../../components/personal-history/personal-history



block main
    .no-critical.no-critical--preloader


block componentsCriticalStyles
    +componentsCriticalStyles-includer()
block vendorScripts
    +vendorScripts-includer()
block componentsStyles
    +style-includer()

block componentsScripts
    +componentsScripts-includer()

block scripts
