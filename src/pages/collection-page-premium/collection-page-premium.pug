extends ../../layout/layout

block vars
    - var pageTitle = 'Карта коллекции - премиум'
    - var pageBodyClass = 'product-page'
    - var dir = './'


block premain
    .breadcrumb-wrap
        .container
            +breadcrumb({
                href_content1: '',
                content1: 'Коллекции',
                href_content2: '',
                content2: 'Skyland',
                content3: ''
            })
    include ../../components/pp-main/pp-main--premium
    include ../../components/pp-tabs/pp-tabs--premium
    include ../../components/contact-form/contact-form--collection
block main
    .no-critical.no-critical--preloader



block componentsCriticalStyles
    +componentsCriticalStyles-includer()
block vendorScripts
    +vendorScripts-includer()
block componentsStyles
    +style-includer()

block componentsScripts
    +componentsScripts-includer()

block scripts
