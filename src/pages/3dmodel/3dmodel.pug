extends ../../layout/layout

block vars
	- var pageTitle = '3-D Модели'
	- var pageBodyClass = ''
	- var dir = './'


block premain
	+page-header-section({
		title: '3-D Модели',
		colClasses: 'col-12',
	})
		+breadcrumb({
			content1: '3-D Модели',
			href_content1: '',
			content2: '',
			href_content2: '',
			content3: '',
		})



block main
	.no-critical.no-critical--preloader
		include ../../components/3dmodels/3dmodels

block componentsCriticalStyles
	+componentsCriticalStyles-includer()
block vendorScripts
	+vendorScripts-includer()
block componentsStyles
	+style-includer()

block componentsScripts
	+componentsScripts-includer()

block scripts
	script(src="./js/vendor/noframework.waypoints.min.js" defer)
