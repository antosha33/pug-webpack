script.
	window.addEventListener( 'load', function( event ) {
		function filter_init() {
			slamFilterJsInit();
			// $('#ajaxfilter-filter').slamAjaxSmartFilter({
			// 	ajaxURL: '/catalog/stiralnye-mashiny/',
			// 	urlDelete: '/catalog/stiralnye-mashiny/',
			// 	align: 'LEFT',
			// 	modeftimeout: 0
			// });
		}

		var filter_init_timeout;


		// Отлаживаем инит на 3с на вью со скрытым фильтром
		if ($(window).width() < 1200) {
			filter_init_timeout = setTimeout(function () {
				filter_init()
			}, 3000)
		} else {
			clearTimeout(filter_init_timeout)
			filter_init()
		}

		let catalogViewBtn = $('.catalog-view__item')
			catalogViewBtn.click(function() {
				let _this = $(this);
				_this.addClass('active')
				_this.siblings().removeClass('active')
				if(_this.hasClass('js-make-list-view')){
					$('.js-catalog-panel').removeClass('catalog-panel--card').addClass('catalog-panel--list')
				}
				if(_this.hasClass('js-make-card-view')){
					$('.js-catalog-panel').removeClass('catalog-panel--list').addClass('catalog-panel--card')
				}
			})


		let filterList = $('.js-filter-list')
		if (filterList.length) {
			filterList.each(function() {
				let _this = $(this);
				let catalogItemCount = _this.children('.form-group').length
				if (catalogItemCount > 6) {
					_this.append('<div class="form-group__link js-link-trigger"><span class="link-show">Показать ещё</span><span class="link-hide">Скрыть</span></div>')
				}
			})
		}

		const showMenu = $('.js-link-trigger')
		showMenu.each(function() {
			let _this = $(this);
			_this.click(function() {
				let list = _this.parent('.js-filter-list')
				_this.toggleClass('active')
				list.toggleClass('opened')
			})
		})

		let targetList = $('.js-target-list')
		if (targetList.length) {
			targetList.each(function() {
				let _this = $(this);
				let targetItemCount = _this.children('.target-link').length
				if (targetItemCount > 4) {
					_this.append('<div class="filter-list-trigger-wrap"><div class="filter-list-trigger js-link-trigger"><span class="link-show">Показать ещё</span><span class="link-hide">Скрыть</span></div></div>')
				}
			})
		}

		const showMenuTarget = $('.js-link-trigger')
		showMenuTarget.each(function() {
			let _this = $(this);
			_this.click(function() {
				let list = _this.parents('.js-target-list')
				_this.toggleClass('active')
				list.toggleClass('opened')
			})
		})


	});
